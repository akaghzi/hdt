<!DOCTYPE html>
<html>
<head>
	<%= render :partial => 'layouts/head' %>
</head>
<body>
	<%= render :partial => 'layouts/header' %>
	<div class="container">
		<div class="pull-right">
			<% if notice %>
			<p class="text-success"><%= notice %></p>
			<% end %>
			<% if alert %>
			<p class="text-danger"><%= alert %></p>
			<% end %>
		</div>
		<%= yield %>
	</div>
	<%= render :partial => 'layouts/footer' %>
</body>
</html>